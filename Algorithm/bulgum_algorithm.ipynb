    "def solution(N, A, B):\n",
    "    A.sort()\n",
    "    idx = sorted(range(N), key=lambda i: B[i], reverse=True)\n",
    "    temp = 0\n",
    "    for j, k in enumerate(idx):\n",
    "        temp += A[j] * B[k]\n",
    "    \n",
    "    return temp"
   ]
